@model IEnumerable<DisneyFilms.Models.Personaje>

@{
    ViewData["Title"] = "age";
}

<h1>Edad</h1>

<p>
    <a asp-action="Create">Create New</a>
    <a asp-action="Index">Back to List</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Edad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Peso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Historia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Imagen)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Edad)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Peso)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Historia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Imagen)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Nombre">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Nombre">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Nombre">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
